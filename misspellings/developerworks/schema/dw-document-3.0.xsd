  <!-- ================================================================ -->
  <!-- headingtype - what kind of heading is this?                      -->
  <!-- ================================================================ -->
  <!-- Can be a major or minor heading, figure heading, code heading, etc. -->
  <xsd:simpleType name="headingtype">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>The classification of a particular heading.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="code">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading (caption) for a code listing</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="figure">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading (caption) for a figure.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="major">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A major heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="minor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A minor heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="resourcelist">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The heading for the list of related resources.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="sidebar">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading for a sidebar.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- inline - should an item be inline or a separate section?         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="inline">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="inline">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This item should be formatted inline.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="section">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This item should be formatted as a separate section.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- level - the level of expertise assumed for this article.         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="level">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>The level of technical expertise assumed for this article.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:positiveInteger">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Limited knowledge, no experience</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>General knowledge, limited experience</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Applied knowledge, can perform routine tasks without assistance</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>In-depth knowledge, can perform all tasks without assitance</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Extensive knowledge and experience</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
