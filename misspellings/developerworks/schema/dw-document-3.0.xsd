  <!-- 10/07 tdc:  Added -->
  <!-- ================================================================ -->
  <!-- subscriptionlevel - the possible developerWorks Subscription levels that users may have.         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="subscriptionlevel">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>The possible developerWorks Subscription levels that users may have.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:positiveInteger">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Guest</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Starter</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Professional</desc>
          </xsd:documentation>
