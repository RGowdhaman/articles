  <xsd:simpleType name="download-type">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="code"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- empty - used for the empty content model                         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="empty">
    <xsd:restriction base="xsd:string">
