<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:dw="http://www.ibm.com/developerWorks/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="2.0">
  <!-- ================================================================ -->
  <!-- Annotations that define other content for our automatically-     -->
  <!-- generated tutorial.                                              -->
  <!-- ================================================================ -->
  <xsd:annotation id="setup">
    <xsd:documentation xml:lang="en">
      <mainTitle>Writing for developerWorks</mainTitle>
      <referenceSectionTitle>Tag Reference</referenceSectionTitle>
      <filename>article</filename>
      <img>images/tutorial.jpg</img>
      <alt>Writing articles for developerWorks</alt>
      <feedback-link>feedback-panel</feedback-link>
      <content-area>XML</content-area>
      <abstract>Want to write an article for developerWorks?  That's great, but you'll need to learn how to use our XML tags.  This tutorial is the documentation for our article tag set, and includes information about setting up tools, writing style, and other useful tips.</abstract>
      <exit_url>http://www.ibm.com/developerWorks/xml/</exit_url>
    </xsd:documentation>
  </xsd:annotation>
  <xsd:annotation id="before">
    <xsd:documentation source="dw-document-before.xml" xml:lang="en"/>
    <xsd:documentation source="editorial-guidelines.xml" xml:lang="en"/>
  </xsd:annotation>
  <xsd:annotation id="after">
    <xsd:documentation source="submitting-your-article.xml" xml:lang="en"/>
    <xsd:documentation source="dw-document-after.xml" xml:lang="en"/>
  </xsd:annotation>
  <!-- ================================================================ -->
  <!-- Elements that define document types                              -->
  <!-- ================================================================ -->
  <!-- <dw-document> element, the root element for everything           -->
  <!-- ================================================================ -->
  <xsd:element name="dw-document">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The root element of all dW content</title>
        <desc>Contains a dW document.  The document is either a <xref refid="dw-article-tag">
            <code>&lt;dw-article&gt;</code>
          </xref>, a <xref refid="dw-lol-tag">
            <code>&lt;dw-lol&gt;</code>
          </xref>, or a <xref refid="dw-sidefile-tag">
            <code>&lt;dw-sidefile&gt;</code>
          </xref> element.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice>
        <xsd:element ref="dw-article"/>
        <xsd:element ref="dw-lol"/>
        <xsd:element ref="dw-sidefile"/>
        <xsd:element ref="dw-subscription-landing"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dw-article> element                                   -->
 <!-- ================================================================ -->
  <xsd:element name="dw-article">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a developerWorks article</title>
        <desc>Contains a developerWorks article.  This is the root element of most documents on the dW site. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="meta" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="productname" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="productfamily" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="content-area" maxOccurs="unbounded"/>
        <xsd:element ref="breadcrumbappend" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="seriestitle" minOccurs="0"/>
        <xsd:element ref="title"/>
        <xsd:element ref="subtitle" minOccurs="0"/>
        <xsd:element ref="forumurl" minOccurs="0"/>
        <xsd:element ref="download-feature" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="pdf" minOccurs="0"/>
        <xsd:element ref="author" maxOccurs="unbounded"/>
        <xsd:element ref="date"/>
        <xsd:element ref="date-updated" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="abstract-extended" minOccurs="0"/>
        <xsd:element ref="column-info" minOccurs="0"/>
        <xsd:element ref="docbody"/>
        <xsd:element ref="attribution" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="relatedlist" minOccurs="0"/>
        <xsd:element ref="resourcelist" minOccurs="0"/>
        <!-- 10/20 lk:  can have multiple download features: -->
      </xsd:sequence>
      <xsd:attribute name="level" type="level"/>
      <xsd:attribute name="localsite" type="site"/>
      <xsd:attribute name="ratings" type="auto" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this article should have a ratings form.  The default value is <code>auto</code>, which means the formatter decides whether the article should have a ratings form or not.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="related_contents" type="auto" use="required"/>
      <xsd:attribute name="toc" type="auto" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this article should have a table of contents.  The default value is <code>auto</code>, which means the formatter decides if the article has enough headings to make a table of contents useful. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dw-lol> element                                                 -->
  <!-- ================================================================ -->
  <xsd:element name="dw-lol">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an <code>lol:&gt;</code> article</title>
        <desc>Contains a developerWorks humor column.  This is the root element of any <code>lol:&gt;</code>documents on the dW site. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id"/>
        <xsd:element ref="meta" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="content-area" maxOccurs="unbounded"/>
        <xsd:element ref="title"/>
        <xsd:element ref="subtitle" minOccurs="0"/>
        <xsd:element ref="seriestitle" minOccurs="0"/>
        <xsd:element ref="author" maxOccurs="unbounded"/>
        <xsd:element ref="date"/>
        <xsd:element ref="date-updated" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="docbody"/>
        <xsd:element ref="attribution" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="relatedlist"/>
      </xsd:sequence>
      <xsd:attribute name="localsite" type="site"/>
      <xsd:attribute name="ratings" type="auto">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this article should have a ratings form.  The default value is <code>auto</code>, which means the formatter decides whether the article should have a ratings form or not.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="related_contents" type="auto"/>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dw-sidefile> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="dw-sidefile">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a sidefile associated with a dW article</title>
        <desc>Contains a sidefile for a developerWorks article.  This is typically rendered as a sidefile within an article. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id"/>
        <xsd:element ref="meta" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="content-area" maxOccurs="unbounded"/>
        <xsd:element ref="productname" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="productfamily" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="breadcrumbappend" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="seriestitle" minOccurs="0"/>
        <xsd:element ref="title"/>
        <xsd:element ref="subtitle" minOccurs="0"/>
        <xsd:element ref="date"/>
        <xsd:element ref="date-updated" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="forumurl" minOccurs="0"/>
        <xsd:element ref="docbody"/>
        <xsd:element ref="attribution" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="localsite" type="site"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 10/07 tdc:  New -->
  <!-- ================================================================ -->
  <!-- <dw-subscription-landing> element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dw-subscription-landing">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a developerWorks Subscription (formerly Toolbox) landing page</title>
        <desc>Contains a developerWorks Subscription landing page.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="meta" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="content-area" maxOccurs="unbounded"/>
        <xsd:element ref="breadcrumbappend" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="seriestitle" minOccurs="0"/>
        <xsd:element ref="title"/>
        <xsd:element ref="subtitle" minOccurs="0"/>
        <xsd:element ref="date" minOccurs="0"/>
        <xsd:element ref="date-updated" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="bundleavailability" minOccurs="0"/>
        <xsd:element ref="bundleoverview"/>
        <xsd:element ref="bundleplatforms" minOccurs="0"/>
        <xsd:element ref="bundlelanguages" minOccurs="0"/>
        <xsd:element ref="bundlemoreabout" minOccurs="0"/>
        <xsd:element ref="bundlerequirements" minOccurs="0"/>
        <xsd:element ref="bundlecomponents"/>
        <xsd:element ref="relatedlist" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="subscriptionlevel" type="subscriptionlevel"/>
      <xsd:attribute name="related_contents" type="yesno" use="required"/>
      <xsd:attribute name="toc" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this developerWorks Subscription landing page should have a table of contents.  The default value is <code>auto</code>, which means the formatter decides if the page has enough headings to make a table of contents useful. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- Elements for content                                             -->
  <!-- ================================================================ -->
  <!-- <a> (anchor) element   -->
  <!-- ================================================================ -->
  <xsd:element name="a">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a hypertext anchor</title>
        <desc>Defines a hypertext link.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="href" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the target URL of this hypertext link. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Gives this location a name.  Named anchors can be referenced just like external URLs.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="onClick" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the action that should take place when the user clicks this link.  This is typically JavaScript code or a call to a JavaScript function. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="origin" type="origin" dw:include="internal">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>!!! Not sure about this one !!!</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="target" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a target for this link.  You can use this attribute to refer to a named browser window.  A value of <code>_blank</code> means the linked document will appear in a new browser window.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="trackclick" type="yesno" default="no" dw:include="internal">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this link should be tracked.  If this attribute has a value of <code>yes</code>, a tracking code will be appended to the link's URL.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
    <!-- Defined as ANY in the DTD -->
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <abstract> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="abstract" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>abstract</title>
        <desc>A short summary of the article, without any formatting tags</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <abstract-extended> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="abstract-extended">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A long summary of the article</title>
        <desc>This element contains a long summary of the article, if needed.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <attribution> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="attribution">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the external source of an article</title>
        <desc>Defines the source (usually an external magazine or Web site) of this article.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="type" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The source of the article.  </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 12/1/03 TDC:  Removed default='javaworld'.  Should probably use an enumeration here in 2.2+ to trigger different attribution statements in xsl. -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <author> element        -->
  <!-- ================================================================ -->
  <xsd:element name="author">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define information about the author of the article</title>
        <desc>This element allows you to define information about the author of this article. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bio"/>
        <xsd:element ref="img" minOccurs="0"/>
        <xsd:element ref="name"/>
      </xsd:sequence>
      <xsd:attribute name="company" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The company the author works for.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="email" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The author's email address.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="jobtitle" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The author's job title.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="email_cc" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>email cc address.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <b> (bold) element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="b">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define boldfaced text</title>
        <desc>Defines boldfaced text. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
        <!-- 4/7/03 Doug added <font> temporarily -->
      </xsd:choice>
      <xsd:attribute name="class" type="colorname">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the color scheme used by this element's text.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <bio> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="bio">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A short biography of the author</title>
        <desc>Defines a short biography of the author.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <blockquote> element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="blockquote">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a lengthy quote</title>
        <desc>Defines a long quote.  The text is indented on the left and right margins.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <br> (break) element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="br">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a line break</title>
        <desc>Defines a line break in the text. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="empty"/>
    </xsd:simpleType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <breadcrumbappend> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="breadcrumbappend">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>An additional link for the breadcrumb trail</title>
        <desc>Allows you to define an additional link for the breadcrumb trail.  This link is appended to the automatically generated links at the top of the HTML version of the document. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="text" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The text of the link. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the link.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- ** Question: Should we have text be optional and use the URL if text isn't there? -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <bundleavailabilityt> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="bundleavailability">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Types of media on which this bundle is available. </title>
        <desc>Types of media on which this bundle is available. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="cd" type="yesno" default="no">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this  bundle is available on CD-ROM</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cdonly" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this  bundle is available only on CD-ROM</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="customcd" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this  bundle is available on a custom CD-ROM.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="web" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this bundle is available on the Web.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="webonly" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this bundle is available only on the Web.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <bundlecomponents> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="bundlecomponents">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Section containing links to multiple developerWorks Subscription bundle component pages.</title>
        <desc>Section containing links to multiple developerWorks Subscription bundle component pages.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="heading"/>
        <xsd:element ref="bundlecomponent" maxOccurs="unbounded"/>
      </xsd:sequence>
      <!-- 11/11 tdc:  removed totalparts attrib since not needed by tbox (info is already contained in original component title) -->
      <!-- <xsd:attribute name="totalparts" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The total number of parts (components) that make up the bundle.  The value for totalparts is the "Y" in the phrase, "Part X of Y".</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute> -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <bundlecomponent> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="bundlecomponent">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A link to a developerWorks Subscription bundle component page.</title>
        <desc>A link to a developerWorks Subscription bundle component page.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="text" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The text of the link. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the link.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 11/11 tdc:  removed installorder and part attrib's since not needed by tbox (info is already contained in original component title) -->
      <!-- <xsd:attribute name="installorder" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The order within the install process that this component should be installed.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="part" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This component is one part out of entire number of parts that make up the bundle.  The value for part is the "X" in the phrase, "Part X of Y".</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute> -->
      <!-- ** Question: Should we have text be optional and use the URL if text isn't there? -->
    </xsd:complexType>
  </xsd:element>
  <!-- 10/07 tdc:  New -->
  <!-- ================================================================ -->
  <!-- <bundlemoreabout> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="bundlemoreabout">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a list of links about a product or bundle</title>
        <desc>Define a list of links about a product or bundle</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="a"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 10/07 tdc:  Added for dw-subscription-landing -->
  <!-- ================================================================ -->
  <!-- <bundleoverview> element                                                -->
  <!-- ================================================================ -->
  <!-- ** Do we want this to contain text?  I don't think so... -->
  <xsd:element name="bundleoverview">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Defines the content of the bundle overview section for developerWorks Subscription pages.</title>
        <desc>Defines the content of the bundle overview section for developerWorks Subscription pages.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="blockquote"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sidebar"/>
        <xsd:element ref="table"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- I removed <dt> and <dd> -->
        <!-- I removed <li> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <code> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="code">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a code listing</title>
        <desc>Defines a code listing.  The contents of this element are displayed in a monospaced font, with all whitespace preserved from the original XML source.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="font"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="type" type="inline" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The type of code listing.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The width in characters of this code listing.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <column-info> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="column-info">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define meta information for this column</title>
        <desc>Defines meta information for this column, such as an icon or a title.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="col-icon" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines an icon for this column.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="col-name" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a name for this column.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- ** This is defined as ANY in the DTD, but it looks like it's empty. -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <date> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="date">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The date this article was created</title>
        <desc>This element defines the date this article was created.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy"/>
    </xsd:complexType>
    <!-- ** Should be an XML Schema date type. -->
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <date-updated> element                                           -->
  <!-- ================================================================ -->
  <xsd:element name="date-updated">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The date this article was updated</title>
        <desc>This element defines the date this article was last updated.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy"/>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dd> (definition) element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="dd">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The definition of a term</title>
        <desc>This element contains the definition of a term in a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dl> (definition list) element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dl">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A definition list</title>
        <desc>Defines a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence maxOccurs="unbounded">
        <xsd:element ref="dt"/>
        <xsd:element ref="dd"/>
      </xsd:sequence>
      <xsd:attribute name="compact" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this definition list should be compacted.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <docbody> element                                                -->
  <!-- ================================================================ -->
  <!-- ** Do we want this to contain text?  I don't think so... -->
  <xsd:element name="docbody">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the content of a document</title>
        <desc>Defines the content of a document.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="blockquote"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sidebar"/>
        <xsd:element ref="table"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- I removed <dt> and <dd> -->
        <!-- I removed <li> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dt> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="dt">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a term</title>
        <desc>Defines a term in a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <figure> element                                                 -->
  <!-- ================================================================ -->
  <xsd:element name="figure">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a illustrative detail</title>
        <desc>Define a code sample, a diagram, or some other illustrative detail.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="heading"/>
        <xsd:element ref="img" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <forumurl> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="forumurl">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Associate a forum with this article</title>
        <desc>Defines the URL of a forum associated with this article. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="url" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the associated forum.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <heading> element                                                -->
  <!-- ================================================================ -->
  <xsd:element name="heading">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a heading</title>
        <desc>Defines the text of a heading.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="alttoc" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines alternate text to be used in the table of contents.  This attribute is useful when the heading has lots of text. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Names this heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="refname" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>????Defines an ID for this heading.  You can use this attribute to reference this heading from elsewhere in the document.???? </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="type" type="headingtype" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the type of heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="toc" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this heading should appear in the table of contents.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <i> (italic) element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="i">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define italicized text</title>
        <desc>Defines text that will appear in an italic font.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <id> element                                                     -->
  <!-- lk added content-id as string to support wsdd and db2 content ratings data -->
  <!-- ================================================================ -->
  <xsd:element name="id">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an ID for a document</title>
        <desc>Defines the Domino document ID for this document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="domino-uid" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the unique Domino document ID for this document.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="content-id" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the unique content id (non-Domino) for this document.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <img> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="img">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an image</title>
        <desc>Defines an image that will appear in the article. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines how the image should be aligned.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="alt" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines alternate text for the graphic.  This text appears in non-graphical browsers and also may be used by accessibility aids such as screen readers.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="border" type="zeroOrOne">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether this image will have a border.  </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="height" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the height in pixels of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="hspace" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the horizontal space around the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a name for the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="src" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the location of the image.  This can be a relative URL or an absolute URL.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign" type="vertical">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="vspace" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical space around the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the width in pixels of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- 10/07 tdc:  Added for dw-subscription-landing -->
  <!-- ================================================================ -->
  <!-- <bundlelanguages> element                                                -->
  <!-- ================================================================ -->
  <!-- ** Do we want this to contain text?  I don't think so... -->
  <xsd:element name="bundlelanguages">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Defines the content of the Languages section for developerWorks Subscription landing pages.</title>
        <desc>Defines the content of the Languages section for developerWorks Subscription pages.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="blockquote"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sidebar"/>
        <xsd:element ref="table"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- I removed <dt> and <dd> -->
        <!-- I removed <li> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <li> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="li">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an item in a list</title>
        <desc>Defines a list item for an ordered or unordered list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="value" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>??? Is this the same as the HTML value attribute?  If so, this changes the item number for this item.  Subsequent list items are numbered accordingly as well. ???</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <meta> (metadata) element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="meta">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define metadata about this article</title>
        <desc>Defines metadata about this article.  (This is used like the HTML <code>&lt;meta&gt;</code> tag.)</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="name" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The name associated with this piece of metadata.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="content" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The metadata itself.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <name> element    -->
  <!-- ================================================================ -->
  <xsd:element name="name">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Author (s) name</title>
        <desc>Defines a the name of an individual author.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <ol> (ordered list) element                                      -->
  <!-- ================================================================ -->
  <xsd:element name="ol">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an ordered list</title>
        <desc>Defines an ordered list.  It contains some number of &lt;li&gt; items.  You can also nest ordered and unordered lists inside the list.</desc>
        <!--, which can in turn contain other lists, such as ordered lists, unordered lists, or definition lists.  In other words, an unordered list can't be a child of an ordered list; you must put the unordered list (at the second level) inside a list item.</desc> -->
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="li"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="ul"/>
      </xsd:choice>
      <xsd:attribute name="start" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the starting number for this list.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="type" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the type of numbering that should be used for this list.  Valid values are:</desc>
            <ul>
              <li>
                <code>1</code> - Items are numbered with Arabic numerals (<code>1</code>, <code>2</code>, <code>3</code>, <code>4</code>, <code>5</code>, and so forth). This is the default numbering style.</li>
              <li>
                <code>A</code> - Items are numbered with uppercase alpha characters (<code>A</code>, <code>B</code>, <code>C</code>, <code>...</code>, <code>AA</code>, <code>AB</code>, <code>AC</code>, and so forth)</li>
              <li>
                <code>a</code> - Items are numbered with lowercase alpha characters (<code>a</code>, <code>b</code>, <code>c</code>, <code>...</code>, <code>aa</code>, <code>ab</code>, <code>ac</code>, and so forth)</li>
              <li>
                <code>I</code> - Items are numbered with uppercase Roman numerals (<code>I</code>, <code>II</code>, <code>III</code>, <code>IV</code>, <code>V</code>, and so forth)</li>
              <li>
                <code>i</code> - Items are numbered with lowercase Roman numerals (<code>i</code>, <code>ii</code>, <code>iii</code>, <code>iv</code>, <code>v</code>, and so forth)</li>
            </ul>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <p> (paragraph) element                                          -->
  <!-- ================================================================ -->
  <xsd:element name="p">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a paragraph</title>
        <desc>Defines a paragraph of text.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
