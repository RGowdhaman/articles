  <!-- ================================================================ -->
  <!-- Attribute Groups                                                 -->
  <!-- ================================================================ -->
  <!-- Day/Month/Year attributes                                        -->
  <!-- ================================================================ -->
  <xsd:attributeGroup name="dmy">
    <xsd:attribute name="day" type="day">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The day of the month the article was created or updated..</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="month" type="month" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The month of the year the article was created or updated.  Do not use a leading zero.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="year" type="recentYear" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The year the article was created or updated. </desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:attributeGroup>
  <!-- ================================================================ -->
  <!-- Data types                                                       -->
  <!-- ================================================================ -->
  <!-- alignment - left, right, or center.                              -->
  <!-- ================================================================ -->
  <xsd:simpleType name="alignment">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="center">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Centers the item horizontally.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="left">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Left-aligns the item horizontally.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="right">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Right-aligns the item horizontally.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- auto - auto, yes, or no.                                         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="auto">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="auto">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Determines the value automatically.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="yes">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The value is set to <code>yes</code>.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="no">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The value is set to <code>no</code>.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- colorname - either blue, green, or red.                          -->
  <!-- ================================================================ -->
  <xsd:simpleType name="colorname">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="blue">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Uses the blue color scheme.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="green">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Uses the green color scheme.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="red">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Uses the red color scheme.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
